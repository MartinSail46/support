<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sz.admin.templatedisk.mapper.TemplateDiskMapper">
    <!-- 通用映射 -->
    <resultMap id="BaseResultMap" type="com.sz.admin.templatedisk.pojo.po.TemplateDisk">
        <id column="id" property="id"/>
        <result column="template_id" property="templateId"/>
        <result column="template_name" property="templateName"/>
        <result column="file_system" property="fileSystem"/>
        <result column="size" property="size"/>
        <result column="used" property="used"/>
        <result column="available" property="available"/>
        <result column="used_Percentage" property="usedPercentage"/>
        <result column="mount_point" property="mountPoint"/>
        <result column="del_flag" property="delFlag"/>
        <result column="revision" property="revision"/>
        <result column="created_id" property="createdId"/>
        <result column="created_time" property="createdTime"/>
        <result column="updated_id" property="updatedId"/>
        <result column="updated_time" property="updatedTime"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        template_id, template_name, file_system, size, used, available, used_Percentage, mount_point, del_flag, revision, created_id, created_time, updated_id, updated_time
    </sql>

</mapper>
